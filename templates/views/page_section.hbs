<div class="container">
	<div class="row">
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			<article>
				<hr>
				<header>

				{{#if data.pages}}
					<h1>{{data.section.title}}</h1>
				{{ else }}
					<p>Coming soon...</p>
				{{/if}}

				</header>
				{{#if data.pages}}
					{{#each data.pages}}
						<div class="intro">
							{{{data.pages.content.intro}}}
						</div>
						<div class="post">
							{{{data.pages.content.body}}}
						</div>
					{{/each}}
				{{/if}}
			</article>
		</div>
	</div>
</div>
