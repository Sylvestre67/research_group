<div class="container">
	<div class="row">
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
			{{#if data.pages}}
				{{#each data.pages}}
					<section>
						<div class="intro">
							{{{content.intro}}}
						</div>
						<div class="post">
							{{{content.body}}}
						</div>
					</section>
				{{/each}}
			{{ else }}
				<p>Coming soon...</p>
			{{/if}}
		</div>
	</div>
</div>