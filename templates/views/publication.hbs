<div class="container">
	<div class="row">
		<div class="col-sm-10 col-sm-offset-1 col-md-8 col-md-offset-2">
		{{#if publications}}
			{{#each publications}}
			<p>{{ title }}</p>
			{{/each}}
		{{ else }}
			<h3 class="text-muted">
				There are no publications yet.
			</h3>
		{{/if}}
		</div>
	</div>
</div>
